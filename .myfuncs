alias gd='gcc -ansi -pedantic-errors -Wall -Wextra -g'
alias gc='gcc -ansi -pedantic-errors -Wall -Wextra -DNDEBUG -O3'
alias gd9='gcc -std=c99 -pedantic-errors -Wall -Wextra -g'
alias gc9='gcc -std=c99 -pedantic-errors -Wall -DNDEBUG -O3'

alias vlg='valgrind --leak-check=yes --track-origins=yes'


alias compall='clear ; gcc -ansi -pedantic-errors -Wall -Wextra -g -Iinclude'
alias runout='./a.out'
alias gitloc='cd ~/git'
alias debug='cgdb ./a.out'

alias goto='bash ~/git/.gdbfunc.sh && cgdb ./a.out -ex=r && source break_points'


#cpp
alias ge='g++ -std=c++98 -pedantic-errors -Wall -Wextra -g'
alias ge9='g++ -std=c++98 -pedantic-errors -Wall -Wextra -o3'
alias cpp98='g++ -std=c++98'
alias cpp11d='g++ -std=c++11 -g'
alias cpp11='g++ -std=c++11'
